		<Param name="DstIndex" dispName="Map Index" varType="int">11</Param>
		<Param name="DstName" dispName="Map Name" varType="string"></Param>
		<Command>xsDisableSelf();}}</Command>

		<Command>rule setup_travel_to_%DstName%</Command>
		<Command>active</Command>
		<Command>highFrequency</Command>
		<Command>{</Command>
			<Command>trEventSetHandler(-%DstIndex%, "travelTo");</Command>
			<Command>xsDisableSelf();</Command>
		<Command>}</Command>

		<Command>rule placeholder_%DstName%_trig_name</Command>
		<Command>inactive</Command>
		<Command>highFrequency</Command>
		<Command>{</Command>
		<Command>if(true) {</Command>
		<Command>xsDisableSelf();</Command>
	</Effect>

</Effects>